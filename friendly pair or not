#include <stdio.h>
int num(int num);
int n1,n2;
int num(int num){
    int s=0;
//scanf("%d",&n);
for(int i=1;i<=num;i++){
       if(num%i==0){
        printf("%d\n ",i);
        s+=i;}}
       printf("sum is %d \n",s);
       return s;
}  
int main()
{
   int n1,n2;
   scanf("%d %d",&n1,&n2);
   
 int s1=num(n1);
 int s2=num(n2);
int t1,t2;
t1=s1/n1;
t2=s2/n2;

if(t1==t2){
        printf("%d %d number pairs are friendly",t1,t2);
    }
else
        printf("numbers are not friendly");
return 0;        
        
}
