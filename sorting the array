void read(float arr[],int n){
	int i;
	printf("The given numbers are:");
	for(i=0<i<n;i++){
		scanf("%f",&a[i]);
	}
}
void sort(float arr[],int n){
float t;
for(int i=0;i<n-1;i++){
	for(int j=i+1;j<n;j++){
		if(arr[i]>arr[j]){
			t=arr[i];
			arr[i]=arr[j];
			arr[i]=t;
			
		}
	}
}
	
}
